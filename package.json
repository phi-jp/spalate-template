{
  "name": "spalate-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "spalate build",
    "start": "spalate start",
    "dev": "spalate build; spalate dev",

    "deploy": "npm run build; gcloud app deploy --project $npm_package_config_stg gae_stg.yaml -q",
    "browse": "gcloud app browse --project $npm_package_config_stg --service default",
    "deploy_prod": "npm run build; gcloud app deploy --project $npm_package_config_prod gae_prod.yaml -q",
    "browse_prod": "gcloud app browse --project $npm_package_config_prod --service default",
    
    "iconfont": "spalate iconfont",
    "browse_icon": "open public/fonts/icons/icons-preview.html",

    "test": ""
  },
  "config": {
    "stg": "{{ project }}_stg",
    "prod": "{{ project }}_prod"
  },
